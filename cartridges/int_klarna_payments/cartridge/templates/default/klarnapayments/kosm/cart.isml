<div class="kosm-cart">
    <isscript>
        var KlarnaOSM = require('~/cartridge/scripts/common/KlarnaOSM');
    </isscript>
    <isif condition="${KlarnaOSM.isEnabledCartPage()}">
        <klarna-placement data-id="${KlarnaOSM.getCartPagePlacementTagId()}" data-purchase_amount="${KlarnaOSM.formatPurchaseAmount(pdict.price)}"></klarna-placement>
        <script src="${KlarnaOSM.getScriptURL()}"></script>
    </isif>
</div>