<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <custom-type type-id="KlarnaCountries">
        <display-name xml:lang="x-default">Klarna Countries</display-name>
        <staging-mode>source-to-target</staging-mode>
        <storage-scope>site</storage-scope>
        <key-definition attribute-id="country">
            <display-name xml:lang="x-default">Country Code</display-name>
            <type>string</type>
            <min-length>0</min-length>
            <field-length>0</field-length>
        </key-definition>
        <attribute-definitions>
            <attribute-definition attribute-id="klarnaLocale">
                <display-name xml:lang="x-default">On-site Messaging Data Default Locale</display-name>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="credentialID">
                <display-name xml:lang="x-default">Service Credential ID</display-name>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="osmUCI">
                <display-name xml:lang="x-default">On-site messaging Data Client ID</display-name>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
			<attribute-definition attribute-id="osmCartEnabled">
                <display-name xml:lang="x-default">Cart Placement Tag Enabled</display-name>
                <description xml:lang="x-default">Whether Cart Placement Tag is Enabled</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
				<default-value>false</default-value>
            </attribute-definition>
			<attribute-definition attribute-id="osmCartTagId">
                <display-name xml:lang="x-default">Cart Placement Data Key</display-name>
                <description xml:lang="x-default">Data Key of On-site messaging placement tag for Cart Page.</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
				<default-value></default-value>
            </attribute-definition>
			<attribute-definition attribute-id="osmPDPEnabled">
                <display-name xml:lang="x-default">PDP Placement Tag Enabled</display-name>
                <description xml:lang="x-default">Whether PDP Placement Tag is Enabled</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
				<default-value>false</default-value>
            </attribute-definition>
			<attribute-definition attribute-id="osmPDPTagId">
                <display-name xml:lang="x-default">PDP Placement Data Key</display-name>
                <description xml:lang="x-default">Data Key of On-site messaging placement tag for PDP Page.</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
				<default-value></default-value>
            </attribute-definition>
            <attribute-definition attribute-id="osmLibraryUrl">
                <display-name xml:lang="x-default">Library URL</display-name>
                <description xml:lang="x-default">URL to On-Site Messaging Library URL</description>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
        </attribute-definitions>
        <group-definitions>
            <attribute-group group-id="custom">
                <display-name xml:lang="x-default">custom</display-name>
                <attribute attribute-id="country"/>
                <attribute attribute-id="klarnaLocale"/>
                <attribute attribute-id="credentialID"/>
                <attribute attribute-id="osmUCI"/>
				<attribute attribute-id="osmCartEnabled"/>
				<attribute attribute-id="osmCartTagId"/>
				<attribute attribute-id="osmPDPEnabled"/>
				<attribute attribute-id="osmPDPTagId"/>
				<attribute attribute-id="osmLibraryUrl"/>
            </attribute-group>
        </group-definitions>
    </custom-type>
</metadata>
