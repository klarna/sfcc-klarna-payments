<?xml version="1.0"?>
<form xmlns="http://www.demandware.com/xml/form/2008-04-19">
	<field formid="state" label="resource.state" type="string" mandatory="false" binding="stateCode" missing-error="address.state.missing" value-error="address.field.invalid">
		<options>
			<option optionid="" 	label="resource.select" 				value=""/>

			<!-- regular CA states -->
			<option optionid="ON" 	label="state.ca.ontario" 				value="ON"/>

			<!-- Other -->
			<option optionid="OTHER" label="state.ca.other" 				value="OTHER"/>
		</options>
	</field>
</form>
