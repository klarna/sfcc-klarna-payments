<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <custom-type type-id="KlarnaCountries">
        <display-name xml:lang="x-default">Klarna Countries</display-name>
        <staging-mode>no-staging</staging-mode>
        <storage-scope>site</storage-scope>
        <key-definition attribute-id="country">
            <display-name xml:lang="x-default">Country Code</display-name>
            <description xml:lang="x-default">Two-letter country code</description>
            <type>string</type>
            <min-length>0</min-length>
            <field-length>0</field-length>
        </key-definition>
        <attribute-definitions>
            <attribute-definition attribute-id="klarnaLocale">
                <display-name xml:lang="x-default">On-site Messaging Data Default Locale</display-name>
                <description xml:lang="x-default">Fallback, if the request locale can’t be dynamically resolved, i.e., when using “default” SFCC locale</description>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="credentialID">
                <display-name xml:lang="x-default">Service Credential ID</display-name>
                <description xml:lang="x-default">The ID of service credentials for this locale</description>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="osmUCI">
                <display-name xml:lang="x-default">On-site messaging Data Client ID</display-name>
                <description xml:lang="x-default">The Klarna On-site Messaging “data-client-id” applicable for a given country</description>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="osmCartEnabled">
                <display-name xml:lang="x-default">Cart Placement Tag Enabled</display-name>
                <description xml:lang="x-default">To enable Cart Placement for a given locale.</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="osmCartTagId">
                <display-name xml:lang="x-default">Cart Placement Data Key</display-name>
                <description xml:lang="x-default">The Klarna On-site Messaging “data-key” of placement applicable for Cart Page for a given locale.</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="osmPDPEnabled">
                <display-name xml:lang="x-default">PDP Placement Tag Enabled</display-name>
                <description xml:lang="x-default">To enable PDP Placement for a given locale.</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="osmPDPTagId">
                <display-name xml:lang="x-default">PDP Placement Data Key</display-name>
                <description xml:lang="x-default">The Klarna On-site Messaging “data-key” of placement applicable for Product Display page for a given locale</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="osmHeaderEnabled">
                <display-name xml:lang="x-default">Header Placement Tag Enabled</display-name>
                <description xml:lang="x-default">To enable Klarna Header Placement in a given storefront</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="osmHeaderTagId">
                <display-name xml:lang="x-default">Header Placement Data Key</display-name>
                <description xml:lang="x-default">The Klarna On-site Messaging “data-key” of placement applicable for the Header</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="osmFooterEnabled">
                <display-name xml:lang="x-default">Footer Placement Tag Enabled</display-name>
                <description xml:lang="x-default">To enable Klarna footer Placement in a given storefront</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="osmFooterTagId">
                <display-name xml:lang="x-default">Footer Placement Data Key</display-name>
                <description xml:lang="x-default">The Klarna On-site Messaging “data-key” of placement applicable for the footer.</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="osmInfoPageEnabled">
                <display-name xml:lang="x-default">Info Page Placement Tag Enabled</display-name>
                <description xml:lang="x-default">To enable Klarna Info Page Placement in a given storefront</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="osmInfoPageTagId">
                <display-name xml:lang="x-default">Info Page Placement Data Key</display-name>
                <description xml:lang="x-default">The Klarna On-site Messaging “data-key” of placement applicable for the Info Page .</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="osmDataInlineEnabled">
                <display-name xml:lang="x-default">Data Inline Enabled on PDP/Cart placement (Canada only)</display-name>
                <description xml:lang="x-default">Enable this when using PayBright payment method in Canada</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="osmLibraryUrl">
                <display-name xml:lang="x-default">URL to On-Site Messaging Library URL</display-name>
                <description xml:lang="x-default">URL to On-Site Messaging Library URL, applicable for testing and production</description>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="kebEnvironment">
                <display-name xml:lang="x-default">Express Button Environment</display-name>
                <type>enum-of-string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">Playground</display>
                        <value>playground</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">Production</display>
                        <value>production</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="kebMerchantID">
                <display-name xml:lang="x-default">Express Button Merchant ID</display-name>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="kebLibraryUrl">
                <display-name xml:lang="x-default">Express Button Library URL</display-name>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="kebCartEnabled">
                <display-name xml:lang="x-default">Express Button Cart Enabled</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="kebTheme">
                <display-name xml:lang="x-default">Express Button Theme</display-name>
                <type>enum-of-string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">Default</display>
                        <value>default</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">Light</display>
                        <value>light</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">Dark</display>
                        <value>dark</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="kebCategory">
                <display-name xml:lang="x-default">Express Button Category</display-name>
                <description xml:lang="x-default">The Klarna Express Button category to be pre-selected on Checkout Page</description>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="paymentMethodID">
                <display-name xml:lang="x-default">Payment Method ID</display-name>
                <description xml:lang="x-default">Payment Method ID that should be used for placing of the order. If not defined, by default the payment method that should be used is 'KLARNA_PAYMENTS'.</description>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="kebMCEnabled">
                <display-name xml:lang="x-default">MiniCart Express Button Enabled</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="kebMCTheme">
                <display-name xml:lang="x-default">MiniCart Express Button Theme</display-name>
                <type>enum-of-string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">Default</display>
                        <value>default</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">Light</display>
                        <value>light</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">Dark</display>
                        <value>dark</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="kebShape">
                <display-name xml:lang="x-default">Express Button Shape</display-name>
                <type>enum-of-string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">Regular</display>
                        <value>default</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">Rectangle</display>
                        <value>rect</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">Pill</display>
                        <value>pill</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="kebMiniCartShape">
                <display-name xml:lang="x-default">Minicart Express Button Shape</display-name>
                <type>enum-of-string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">Regular</display>
                        <value>default</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">Rectangle</display>
                        <value>rect</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">Pill</display>
                        <value>pill</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="expressCheckoutClientKey">
                <display-name xml:lang="x-default">Klarna Client Identifier</display-name>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
        </attribute-definitions>
        <group-definitions>
            <attribute-group group-id="custom">
                <display-name xml:lang="x-default">General</display-name>
                <attribute attribute-id="country"/>
                <attribute attribute-id="klarnaLocale"/>
                <attribute attribute-id="credentialID"/>
                <attribute attribute-id="paymentMethodID"/>
            </attribute-group>
            <attribute-group group-id="osm">
                <display-name xml:lang="x-default">On-site Messaging</display-name>
                <attribute attribute-id="osmUCI"/>
                <attribute attribute-id="osmCartEnabled"/>
                <attribute attribute-id="osmCartTagId"/>
                <attribute attribute-id="osmPDPEnabled"/>
                <attribute attribute-id="osmPDPTagId"/>
                <attribute attribute-id="osmHeaderEnabled"/>
                <attribute attribute-id="osmHeaderTagId"/>
                <attribute attribute-id="osmFooterEnabled"/>
                <attribute attribute-id="osmFooterTagId"/>
                <attribute attribute-id="osmInfoPageEnabled"/>
                <attribute attribute-id="osmInfoPageTagId"/>
                <attribute attribute-id="osmDataInlineEnabled"/>
                <attribute attribute-id="osmLibraryUrl"/>
            </attribute-group>
            <attribute-group group-id="expressButton">
                <display-name xml:lang="x-default">Express Button</display-name>
                <attribute attribute-id="kebMerchantID"/>
                <attribute attribute-id="kebEnvironment"/>
                <attribute attribute-id="kebTheme"/>
                <attribute attribute-id="kebShape"/>
                <attribute attribute-id="kebCartEnabled"/>
                <attribute attribute-id="kebLibraryUrl"/>
                <attribute attribute-id="kebCategory"/>
            </attribute-group>
            <attribute-group group-id="expressMCButton">
                <display-name xml:lang="x-default">MiniCart Express Button</display-name>
                <attribute attribute-id="kebMCEnabled"/>
                <attribute attribute-id="kebMCTheme"/>
                <attribute attribute-id="kebMiniCartShape"/>
            </attribute-group>
            <attribute-group group-id="expressCheckout">
                <display-name xml:lang="x-default">Express Checkout</display-name>
                <attribute attribute-id="expressCheckoutClientKey"/>
            </attribute-group>
        </group-definitions>
    </custom-type>
</metadata>
